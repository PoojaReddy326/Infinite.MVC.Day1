@model List<Infinite.MVC.Day1.Models.Product>
@{
    ViewBag.Title = "Index";
}

<h2>Product Index</h2>

<h2>List of Products</h2>
<hr />

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Qunatity</th>
    </tr>

    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.ProductName</td>
            <td>@p.Price</td>
            <td>@p.Quantity</td>
        </tr>

    }
</table>
